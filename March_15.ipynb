   "source": [
    "#factorial function\n",
    "#not the cleanest but does the job\n",
    "F = 1\n",
    "i = 1\n",
    "n = int(input('enter a positive integer'))\n",
    "\n",
    "if n == 0:\n",
    "    print('0!=1')\n",
    "    exit()\n",
    "else:\n",
    "    while i <= n:\n",
    "        F = F*i\n",
    "        i+=1\n",
    "\n",
    "if n!=0:\n",
    "    print(n,'!=',F)\n"
   ]
